@using System.Threading
@{
    var titleTag = (string.IsNullOrWhiteSpace(ViewBag.Title) ? "" : ViewBag.Title + " - ") + Resources.Common.ApplicationName;


    var asd = ViewBag.asd;

    // set up global js variable, to hold culture code
    // it is later used for globalize, cldr and moment
    var currentCultureCode = "root";
    switch (Thread.CurrentThread.CurrentCulture.ToString())
    {
        case "et-EE":
            currentCultureCode = "et";
            break;
        case "en-US":
            currentCultureCode = "en";
            break;
        default:
            currentCultureCode = Thread.CurrentThread.CurrentCulture.ToString();
            break;
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@titleTag</title>

    <link href="~/Content/flags.css" rel="stylesheet"/>

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="/Content/bootstrap.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/Content/AdminTheme/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
    folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="/Content/AdminTheme/css/skins/_all-skins.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="/Scripts/AdminTheme/plugins/iCheck/flat/blue.css">
    <!-- Morris chart -->
    <link rel="stylesheet" href="/Scripts/AdminTheme/plugins/morris/morris.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="/Scripts/AdminTheme/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="/Scripts/AdminTheme/plugins/datepicker/datepicker3.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="/Scripts/AdminTheme/plugins/daterangepicker/daterangepicker-bs3.css">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="/Scripts/AdminTheme/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="~/Scripts/AdminTheme/plugins/select2/select2.css" rel="stylesheet" />

    @RenderSection("AdminStyles", required: false)
</head>

<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index", "Home")" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>@Resources.Common.G</b>@Resources.Common.M</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>@Resources.Common.Gentle</b>@Resources.Common.Man</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            @Html.Partial("_AdminNavBarPartial")
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("_AdminLeftSideBarPartial")


        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.Title
                </h1>
            </section>
            <!-- Main content -->

            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>

        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Culture:</b>@Culture <b>UICulture:</b>@UICulture
            </div>
            @*<strong>Copyright &copy; 2014-2015 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights
            reserved.*@
        </footer>

    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.0 -->
    <script src="/Scripts/AdminTheme/plugins/jQuery/jQuery-2.2.0.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/cldr")
    @Scripts.Render("~/bundles/globalize")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/moment")

    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.6 -->
    <script src="/Scripts/bootstrap.js"></script>
    <!-- Morris.js charts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="/Scripts/AdminTheme/plugins/morris/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="/Scripts/AdminTheme/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="/Scripts/AdminTheme/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="/Scripts/AdminTheme/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="/Scripts/AdminTheme/plugins/knob/jquery.knob.js"></script>
    <!-- daterangepicker -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
    <script src="/Scripts/AdminTheme/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="/Scripts/AdminTheme/plugins/datepicker/bootstrap-datepicker.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="/Scripts/AdminTheme/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="/Scripts/AdminTheme/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="/Scripts/AdminTheme/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="/Scripts/AdminTheme/js/app.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="/Scripts/AdminTheme/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
<script src="/Scripts/AdminTheme/js/demo.js"></script>
    <script src="~/Scripts/AdminTheme/plugins/select2/select2.full.js"></script>
<script>
    $(function() {
        //Initialize Select2 Elements
        $(".select2").select2();
    });
</script>
    


    @RenderSection("AdminScripts", required: false)
</body>
</html>

