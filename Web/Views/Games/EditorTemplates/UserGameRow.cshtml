@model Domain.Models.UserGameRow

<div class="form-group form-inline" data-userid="@Model.UserIntId">
    @Html.TextBoxFor(model => model.StartBet, new { htmlAttributes = new { @class = "form-control pull-center table_tb start"} })
    @Html.TextBoxFor(model => model.EndBet, new { htmlAttributes = new { @class = "form-control pull-center table_tb end"} })
    @Html.ValidationMessageFor(model => model.StartBet, "", new { @class = "text-danger" })
    @Html.ValidationMessageFor(model => model.EndBet, "", new { @class = "text-danger" })
</div>
